; $Id: Version,fff,v 1.1.2.3 1999-01-12 00:27:30 ds Exp $

; Set test_flag for alpha and beta versions.
; Set patch_level for patches on major releases.

; build_name *must* be defined on the assembler's command line. Examples:
;  -Dbuild_name         -> "patch-<patch_level>" (null if patch_level="0")
;  -Dbuild_name=foo     -> "foo-<patch_level>"
;  -Dbuild_name=foo-42  -> "foo-42"

	GBLS	version
	GBLS	patch_level
	GBLA	version_major
	GBLA	version_minor
	GBLA	version_config
	GBLL	test_flag

version		SETS "1.41 "
patch_level	SETS "1"
version_major	SETA 1
version_minor	SETA 41
version_config	SETA 0
test_flag	SETL {TRUE}

