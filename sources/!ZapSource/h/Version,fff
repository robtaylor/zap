	GBLS	version
	GBLS	patch_level
	GBLA	version_major
	GBLA	version_minor
	GBLA	version_config
	GBLL	test_flag

version		SETS "1.41 "
patch_level	SETS "1"
version_major	SETA 1
version_minor	SETA 41
version_config	SETA 0
test_flag	SETL {TRUE}

