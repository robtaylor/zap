PC		RN	15

;  Status register bit masks

Fbit		EQU	1:SHL:26
Ibit		EQU	1:SHL:27
Vbit		EQU	1:SHL:28
Cbit		EQU	1:SHL:29
Zbit		EQU	1:SHL:30
Nbit		EQU	1:SHL:31

;  SWI numbers for RISC OS

Xbit						*	&20000

	MACRO
$l	dSWI	$n
$l	*	&$n
X$l	*	(&$n:OR:Xbit)
	MEND

	MACRO
$l	dZSWI	$n
	[ UseZapXRedraw
$l	*	&$n + &48440			; ZapXRedraw base number
X$l	*	(&$n + &48440 :OR: Xbit)
	|
$l	*	&$n + &48480			; ZapRedraw base number
X$l	*	(&$n + &48480 :OR: Xbit)
	]
	MEND

	[ :LNOT::DEF: UseZapXRedraw
		GBLL UseZapXRedraw
UseZapXRedraw	SETL {FALSE}
	]

