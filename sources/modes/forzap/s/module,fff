	LDR	r7, isentity
	TST	r7, #0x100		; this bit is always clear
	BLEQ	maker0upper
	MOV	r2, r0
findnextincc
	LDRB	r1, [r11, r12]		; read char from table
	CMP	r1, #'#'		; case sensitive flag?
	ADDEQ	r11, r11, #1
	BEQ	findnextincc
	CMP	r1, #'{'		; end of table entry?
	BCS	fndcmdend2		; yup
	LDR	r7, isentity
	TST	r7, #0x100
	BLEQ	maker1upper
	CMP	r1, r2
	BNE	fndcmdend
