Set prefix$dir <ZapExtSource$Dir>.Text
If "<ZapText$Dir>" = "" Then Error 0 Please locate ZapText
AMU -f <ZapExtSource$Dir>.Text.MakeFile

Set prefix$dir <ZapExtSource$Dir>.Util
If "<ZapUtil$Dir>" = "" Then Error 0 Please locate ZapUtil
AMU -f <ZapExtSource$Dir>.Util.MakeFile

|Set prefix$dir <ZapExtSource$Dir>.Prog
|If "<ZapProg$Dir>" = "" Then Error 0 Please locate ZapProg
|AMU -f <ZapExtSource$Dir>.Prog.MakeFile

|Set prefix$dir <ZapExtSource$Dir>.Bits
|If "<ZapBits$Dir>" = "" Then Error 0 Please locate ZapBits
|AMU -f <ZapExtSource$Dir>.Bits.MakeFile
