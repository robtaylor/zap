gotoopenwindow
	FNJSR
	BL	openwindowroutine
	CMP	r0, #0			;   If there was a pane then no problem
	FNRTS	NE			;   just return...
	BL	gomakepanehappen	;   ...otherwise try to make one...
	BL	openwindowroutine	;   ...and redraw it...
	FNRTS

openwindowroutine
	FNJSR	"R0-R12"
	ADRL	lr, w_formmb
	LDR	lr, [lr, #8]
	TST	lr, #(1 << 20)
	TSTEQ	lr, #(1 << 31)
	FNRTS	EQ
	BL	trytoloadmode
	ADRL	r0, buttonsamed2
	FNcall	Zap_TestModeName
	CMP	r0, #1
	BMI	_altered_11
	MOV	r0, #18
	ADR	r1, buttonsamed
	SWI	XOS_Module
	BVS	_altered_11
	ADD	r3, r3, #0x28
	STR	r3, alocaljumper
	FNPULL
		;     FNlong_adr("  ",2,w_formmb)
		;     LDR     2,[2,#8]
		;    AndS    2,2,#1 << 31
		;   MOVNE   2,#&FF
	LDR	pc, alocaljumper

