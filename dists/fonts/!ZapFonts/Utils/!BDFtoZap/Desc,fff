# This is a description of the wimp interface for BDF->Zap

tool_details_start
   name          "BDFtoZap";
   command_is    "<BDFtoZap$Dir>.bdf->zap";
   version       "1.01";
   wimpslot       320k;
   has_extended_cmdline;
tool_details_end


metaoptions_start
   has_text_window;
   has_summary_window;
metaoptions_end


dbox_start
   icons_start
      icn 3 maps_to string not_saved;
      icn 5 maps_to string not_saved;
      icn 7 maps_to string not_saved;
   icons_end

   defaults
   icn 3 "",
   icn 5 "";

   imports_start
      drag_to  icn 3   inserts icn 3;
      drag_to  icn 5   inserts icn 5;
      drag_to  icn 7   inserts icn 7;
      drag_to  iconbar inserts icn 3;
   imports_end
dbox_end

