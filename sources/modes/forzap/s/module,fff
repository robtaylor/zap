	BLT	_altered_5
		; STMFD	  R13!,{R8-R12}
		;
	STR	r12, delhash		; length already present
	MOV	r7, r11			; start of table entry
	MOV	r12, #0
loop4					; count length of table entry
	ADD	r12, r12, #1
	LDRB	r1, [r11, r12]
	CMP	r1, #' '
	CMPNE	r1, #'{'
	BLT	loop4
	CMP	r1, #' '
	ADDEQ	r11, r11, r12
	LDREQB	r1, [r11, #1]
