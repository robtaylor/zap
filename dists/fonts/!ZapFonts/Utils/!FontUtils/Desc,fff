# This is a description of the WIMP interface for FontUtils

tool_details_start
   name          "FontUtils";
   command_is    "<FontUtils$Dir>.Library.FontU_TW";
   version       "1.00";
   wimpslot       160k;
   has_extended_cmdline;
tool_details_end


metaoptions_start
   has_text_window;
   has_summary_window;
   display_dft_is text;
metaoptions_end


dbox_start
   icons_start
      icn  6 maps_to "-width";
      icn  7 maps_to string not_saved;
      icn  8 maps_to "-height";
      icn  9 maps_to string not_saved;
      icn 10 maps_to "-xoffset";
      icn 11 maps_to string not_saved;
      icn 12 maps_to "-yoffset";
      icn 13 maps_to string not_saved;
      icn 14 maps_to "-90x90dpi";
      icn 15 maps_to "-90x45dpi";
      icn 16 maps_to "-bold";
      icn 17 maps_to "-italic";
      icn 18 maps_to "-underline";
      icn 19 maps_to "-stylesize";
      icn 20 maps_to string not_saved;
      icn 21 maps_to "-stylepos";
      icn 22 maps_to string not_saved;
      icn 23 maps_to "-first";
      icn 24 maps_to string not_saved;
      icn 25 maps_to "-last";
      icn 26 maps_to string not_saved;
      icn 28 maps_to "-magx";
      icn 29 maps_to string not_saved;
      icn 30 maps_to "-magy";
      icn 31 maps_to string not_saved;
      icn 32 maps_to string not_saved;
      icn 33 maps_to string not_saved;
   icons_end

   defaults
   icn 7  "",
   icn 9  "",
   icn 11 "",
   icn 13 "",
   icn 20 "",
   icn 22 "",
   icn 24 "",
   icn 26 "",
   icn 29 "",
   icn 31 "",
   icn 32 "",
   icn 33 "";

   imports_start
      drag_to  icn 32  inserts icn 32;
      drag_to  icn 33  inserts icn 33;
      drag_to  iconbar inserts icn 32;
   imports_end
dbox_end
