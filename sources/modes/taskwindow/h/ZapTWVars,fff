			^ 0

buf_insert		# 4		; v 0   buffer used to insert via e_char
buffer			# 4		; v 256 general purpose buffer

mode_taskwindow		# 4		; mode number assigned to taskwindow mode

taskw_str		# 256		; RMA buffer of TaskWindow command

variable_size		* @


; bits in the mode word

mw_bracket_match	*	1<<31
mw_die_silently		*	1<<30

