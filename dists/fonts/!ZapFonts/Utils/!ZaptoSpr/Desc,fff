# This is a description of the wimp interface for ZaptoSpr

tool_details_start
   name          "ZaptoSpr";
   command_is    "<ZaptoSpr$Dir>.CnvrtFonts";
   version       "0.01";
   wimpslot       320k;
tool_details_end


metaoptions_start
   has_text_window;
   has_summary_window;
metaoptions_end


dbox_start
   icons_start
      icn 3 maps_to string not_saved;
      icn 5 maps_to string not_saved;
      icn 6 maps_to string not_saved;
   icons_end

   defaults
   icn 3 "",
   icn 5 "",
   icn 6 "1";

   imports_start
      drag_to  icn 3   inserts icn 3;
      drag_to  icn 5   inserts icn 5;
      drag_to  iconbar inserts icn 3;
   imports_end
dbox_end

